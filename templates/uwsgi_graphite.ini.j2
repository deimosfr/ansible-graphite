[uwsgi]
processes = {{uwsgi_processes}}
socket = {{uwsgi_socket}}
chmod-socket = {{uwsgi_chmod_socket}}
gid = {{uwsgi_uid}}
uid = {{uwsgi_gid}}
chdir = {{uwsgi_chdir}}
module = {{uwsgi_module}}
pythonpath = /opt/graphite/webapp/
home = {{ graphite_install_dir }}
{% if uwsgi_thunder_lock %}
thunder-lock = true
{% endif %}
{% if uwsgi_enable_threads %}
enable-threads = true
{% endif %}
